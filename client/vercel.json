{
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "client/dist/index.html"
    }
  ],
  "env": {
    "VITE_APP_TITLE": "Tosati Advocacia",
    "VITE_APP_LOGO": "https://i.ibb.co/mV70qmxv/LOGO3-copiar.png"
  }
}
